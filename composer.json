{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 7 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "composer/installers": "^1.2",
        "composer/semver": "^1.4",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/preserve-paths": "^0.1",
        "drupal/admin_views": "^1.6",
        "drupal/adminimal_admin_menu": "^1.9",
        "drupal/administerusersbyrole": "^2.0",
        "drupal/auto_nodetitle": "^1.0",
        "drupal/backup_migrate": "^3.6",
        "drupal/better_exposed_filters": "^3.6",
        "drupal/calendar": "^3.5",
        "drupal/ckeditor": "^1.18",
        "drupal/composer_autoloader": "^1.0",
        "drupal/content_access": "^1.2@beta",
        "drupal/ctools": "^1.15",
        "drupal/date": "^2.10",
        "drupal/datepicker": "^1.0",
        "drupal/drupal": "^7.62",
        "drupal/entity": "^1.9",
        "drupal/fe_block": "^1.0",
        "drupal/features": "^2.11",
        "drupal/field_group": "^1.6",
        "drupal/field_permissions": "^1.0",
        "drupal/front_page": "^2.4",
        "drupal/i18n": "^1.26",
        "drupal/i18n_field": "^1.26",
        "drupal/i18n_string": "^1.26",
        "drupal/imce": "^1.11",
        "drupal/jquery_update": "^2.7",
        "drupal/l10n_update": "^2.2",
        "drupal/libraries": "^2.5",
        "drupal/link": "^1.6",
        "drupal/linkit": "^3.5",
        "drupal/media": "^2.21",
        "drupal/menu_firstchild": "^1.1",
        "drupal/menu_item_visibility": "^1.0@beta",
        "drupal/metatag": "^1.25",
        "drupal/module_filter": "^2.2",
        "drupal/pathauto": "^1.3",
        "drupal/pathologic": "^3.1",
        "drupal/pathrules": "^1.0",
        "drupal/profile2": "^1.6",
        "drupal/profile2_regpath": "^1.13",
        "drupal/references": "^2.2",
        "drupal/references_uuid": "^2.2",
        "drupal/registration": "^1.7",
        "drupal/registration_entity_access": "^1.7",
        "drupal/registration_views": "^1.0",
        "drupal/registry_rebuild": "^2.5",
        "drupal/rules": "^2.12",
        "drupal/rules_admin": "^2.12",
        "drupal/strongarm": "^2.0",
        "drupal/token": "^1.7",
        "drupal/user_picture_default": "^1.0@beta",
        "drupal/uuid": "^1.2",
        "drupal/uuid_path": "^1.2",
        "drupal/uuidrole": "^1.3",
        "drupal/variable": "^2.5",
        "drupal/variable_admin": "^2.5",
        "drupal/views": "^3.22",
        "drupal/views_bulk_operations": "^3.5",
        "drupal/views_ui": "^3.22",
        "drupal/vppr": "^1.0",
        "drupal/workflow": "^2.11",
        "drupal/workflow_access": "^2.11",
        "drupal/workflow_admin_ui": "^2.11",
        "drupal/workflow_rules": "^2.11",
        "drupal/workflow_vbo": "^2.11",
        "drupal/workflowfield": "^2.11",
        "drush/drush": "^8.0",
        "symfony/filesystem": "~2.7|^3",
        "webflo/drupal-finder": "^1.0.0"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::removeInternalFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/": ["type:drupal-core"],
            "web/profiles/{$name}/": ["type:drupal-profile"],
            "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
            "web/profiles/custom/band_booking/libraries/{$name}/": ["type:drupal-library"],
            "web/profiles/custom/band_booking/modules/contrib/{$name}/": ["type:drupal-module"],
            "web/profiles/custom/band_booking/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/band_booking/themes/contrib/{$name}/": ["type:drupal-theme"]
        },
        "patches": {
            "cweagans/composer-patches": {
                "Call the preserve paths hooks" : "https://github.com/SebCorbin/composer-patches/commit/1cb9bacba51f8bba0c5f3f322c3bb61e4ceae974.patch"
            }
        },
        "preserve-paths": [
            "web/sites",
            "web/profiles/custom/band_booking/modules/custom",
            "web/profiles/custom/band_booking/themes/custom",
            "web/profiles/custom/band_booking/translations"
        ]
    },
    "require-dev": {
        "drupal/devel": "^1.7",
        "drupal/diff": "^3.4",
        "drupal/features_diff": "^1.0@beta",
        "drupal/search_krumo": "^1.6"
    }
}
