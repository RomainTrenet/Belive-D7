# Ignore directories generated by Composer
/vendor/
/web/sites/all/drush/
/web/profiles/band_booking/modules/contrib/
/web/profiles/band_booking/themes/contrib/

# Ignore Drupal 7 core files.
#/web/*
#!/web/*/
/web/install.php
/web/update.php
/web/index.php
/web/includes/
/web/misc/
/web/modules/
/web/profiles/README.txt
/web/profiles/minimal/
/web/profiles/standard/
/web/profiles/testing/
/web/scripts/
/web/sites/README.txt
/web/sites/all/*/README.txt
/web/sites/example.sites.php
/web/themes/

# Ignore configuration files that may contain sensitive information.
/web/sites
/web/sites/*/*.settings.php
/web/sites/*/settings*.php
/web/sites/sites.php

# Ignore paths that contain user-generated content.
/web/sites/*/files
/web/sites/*/private

# Ignore temporary folder
/web/tmp/

# Ignore files generated by PhpStorm
/web/.idea/
/.idea/

# Ignore MAC OS files
/*/.DS_Store
/.DS_Store
*/.DS_Store
.DS_Store
